# AWS Region dove sono configurati i servizi (es. us-east-1, eu-west-1, ecc.)
AWS_REGION=

# ID dell'app client Cognito. Si trova nella sezione "App client settings" del tuo User Pool in AWS Cognito
COGNITO_CLIENT_ID=

# ID del Cognito User Pool. Visibile nella dashboard del tuo User Pool in AWS Cognito
USER_POOL_ID=

# Segreto usato per firmare e verificare i JWT. Inserisci un valore alfanumerico random lungo (es. generato con openssl o un password manager)
JWT_SECRET=

# Nome utente del database RDS (MySQL, PostgreSQL, ecc.)
RDS_USERNAME=

# Endpoint del database RDS, es. example-instance.xxxxxxxx.region.rds.amazonaws.com
RDS_HOSTNAME=

# Nome del database all'interno della tua istanza RDS
RDS_DB_NAME=

# Password del database RDS
RDS_PASSWORD=

# Porta del database RDS (di solito 3306 per MySQL, 5432 per PostgreSQL)
RDS_PORT=

# API Key di Gemini (Google AI). Puoi ottenerla dalla console Google AI Studio: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=
